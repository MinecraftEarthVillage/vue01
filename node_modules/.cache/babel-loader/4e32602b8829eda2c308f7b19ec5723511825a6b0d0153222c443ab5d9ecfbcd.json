{"ast":null,"code":"import { reactive, toRefs, onBeforeMount, onMounted, onBeforeUpdate, onUpdated, onBeforeUnmount, onUnmounted, onDeactivated } from 'vue';\nexport default {\n  name: \"App\",\n  setup() {\n    console.log(\"1...setup()开始创建组件\");\n    const data = reactive({\n      arr: [\"Yes\", \"NO\"],\n      selectOne: \"\",\n      selectOneFun: index => {\n        data.selectOne = data.arr[index];\n      }\n    });\n    onBeforeMount(() => {\n      console.log(\"2...onBeforeMount()组件挂在到页面之前执行\");\n      onMounted(() => {\n        console.log(\"3...onMounted()组件挂在到页面之后执行\");\n      });\n      onBeforeUpdate(() => {\n        console.log(\"4..toExponential.onBeforeUpdate()在组件更新之前执行\");\n      });\n      onUpdated(() => {\n        console.log(\"5...onUpdated()在组件更新之后执行\");\n      });\n      onBeforeUnmount(() => {\n        console.log(\"6...onBeforeUnmount()在组件卸载之前执行\");\n      });\n      onUnmounted(() => {\n        console.log(\"7...onUnmounted()在组件卸载之后执行\");\n      });\n      onDeactivated(() => {\n        console.log(\"8...onDeactivated()在组件切换中老组件消失的时候执行\");\n      });\n      const refData = toRefs(data);\n      return {\n        ...refData\n      };\n    });\n  }\n};","map":{"version":3,"names":["reactive","toRefs","onBeforeMount","onMounted","onBeforeUpdate","onUpdated","onBeforeUnmount","onUnmounted","onDeactivated","name","setup","console","log","data","arr","selectOne","selectOneFun","index","refData"],"sources":["C:\\Users\\Administrator\\Desktop\\vue01\\src\\components\\test13-1.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <button v-for=\"(item,index) in arr\" v-bind:key=\"index\" @click=\"selectOneFun(index)\">\r\n        {{ index }}:{{ item }}\r\n        </button>\r\n    </div>\r\n    <button v-for=\"(item,index) in arr\" v-bind:key=\"index\" @click=\"selectOneFun(index)\"></button>\r\n    <div>你选择了【{{ selectOne }}】</div>\r\n</template>\r\n\r\n<script>\r\nimport { reactive,toRefs,onBeforeMount,onMounted, onBeforeUpdate,  onUpdated ,onBeforeUnmount,onUnmounted,onDeactivated} from 'vue';\r\n\r\n\r\nexport default {\r\n    name: \"App\",\r\n    setup(){\r\n        console.log(\"1...setup()开始创建组件\");\r\n        const data=reactive({\r\n            arr: [\"Yes\",\"NO\"],\r\n            selectOne:\"\",\r\n            selectOneFun:(index)=>{data.selectOne=data.arr[index];},\r\n        });\r\n        onBeforeMount(  ()=>    {\r\n            console.log(\"2...onBeforeMount()组件挂在到页面之前执行\");\r\n            onMounted(()=>{\r\n                console.log(\"3...onMounted()组件挂在到页面之后执行\")\r\n            })\r\n            onBeforeUpdate(()=>{\r\n                console.log(\"4..toExponential.onBeforeUpdate()在组件更新之前执行\");\r\n            \r\n            })\r\n\r\n            onUpdated(()=>{\r\n                console.log(\"5...onUpdated()在组件更新之后执行\");\r\n            })\r\n\r\n            onBeforeUnmount(()=>{\r\n                console.log(\"6...onBeforeUnmount()在组件卸载之前执行\");\r\n            })\r\n            onUnmounted(()=>{\r\n                console.log(\"7...onUnmounted()在组件卸载之后执行\");\r\n            });\r\n            onDeactivated(()=>{\r\n                console.log(\"8...onDeactivated()在组件切换中老组件消失的时候执行\");\r\n            })\r\n            const refData=toRefs(data);\r\n            return{...refData};\r\n        })\r\n    }\r\n}\r\n</script>\r\n"],"mappings":"AAWA,SAASA,QAAQ,EAACC,MAAM,EAACC,aAAa,EAACC,SAAS,EAAEC,cAAc,EAAGC,SAAQ,EAAGC,eAAe,EAACC,WAAW,EAACC,aAAa,QAAO,KAAK;AAGnI,eAAe;EACXC,IAAI,EAAE,KAAK;EACXC,KAAKA,CAAA,EAAE;IACHC,OAAO,CAACC,GAAG,CAAC,mBAAmB,CAAC;IAChC,MAAMC,IAAI,GAACb,QAAQ,CAAC;MAChBc,GAAG,EAAE,CAAC,KAAK,EAAC,IAAI,CAAC;MACjBC,SAAS,EAAC,EAAE;MACZC,YAAY,EAAEC,KAAK,IAAG;QAACJ,IAAI,CAACE,SAAS,GAACF,IAAI,CAACC,GAAG,CAACG,KAAK,CAAC;MAAC;IAC1D,CAAC,CAAC;IACFf,aAAa,CAAG,MAAQ;MACpBS,OAAO,CAACC,GAAG,CAAC,gCAAgC,CAAC;MAC7CT,SAAS,CAAC,MAAI;QACVQ,OAAO,CAACC,GAAG,CAAC,4BAA4B;MAC5C,CAAC;MACDR,cAAc,CAAC,MAAI;QACfO,OAAO,CAACC,GAAG,CAAC,4CAA4C,CAAC;MAE7D,CAAC;MAEDP,SAAS,CAAC,MAAI;QACVM,OAAO,CAACC,GAAG,CAAC,0BAA