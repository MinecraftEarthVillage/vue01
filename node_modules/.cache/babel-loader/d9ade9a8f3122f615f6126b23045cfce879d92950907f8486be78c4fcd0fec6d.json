{"ast":null,"code":"import { reactive, toRefs, computed } from 'vue';\nimport tabhome from './TabHome.vue';\nimport tabposts from './TabPosts.vue';\nimport tabarchive from './TabArchive.vue';\nexport default {\n  components: {\n    tabhome,\n    tabarchive,\n    tabposts\n  },\n  setup() {\n    const state = reactive({\n      currentTab: 0,\n      tabsCompName: ['tabhome', 'tabposts', 'tabarchive'],\n      tabsName: ['学校主页', '大千世界', '校内档案']\n    });\n    const currentTabComponent = computed(() => {\n      return state.tabsCompName[state.currentTab];\n    });\n    return {\n      ...toRefs(state),\n      currentTabComponent,\n      tabhome,\n      tabposts,\n      tabarchive\n    };\n  }\n};","map":{"version":3,"names":["reactive","toRefs","computed","tabhome","tabposts","tabarchive","components","setup","state","currentTab","tabsCompName","tabsName","currentTabComponent"],"sources":["D:\\网页设计\\Vue\\vue01\\src\\实验8\\TabFather.vue"],"sourcesContent":["<template>\r\n    <div id=\"dynamic-component-demo\" class=\"demo\">\r\n        <button v-for=\"(tab,index) in tabsName\" v-bind:key=\"index\" v-bind:class=\"['tab-button',{active:currentTab===index}]\" v-on:click=\"currentTab=index\">\r\n        {{ tab }}\r\n        </button>\r\n        <component v-bind:is=\"currentTabComponent\" class=\"tab\"></component>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { reactive,toRefs,computed } from 'vue'\r\nimport tabhome from './TabHome.vue'\r\nimport tabposts from './TabPosts.vue'\r\nimport tabarchive from './TabArchive.vue'\r\n\r\nexport default{\r\n    components:{\r\n        tabhome,tabarchive,tabposts\r\n    },\r\n    setup(){\r\n        const state =reactive({\r\n            currentTab:0,\r\n            tabsCompName:['tabhome','tabposts','tabarchive'],\r\n            tabsName:['学校主页','大千世界','校内档案']\r\n        })\r\n        const currentTabComponent=computed(()=>{return state.tabsCompName[state.currentTab]})\r\n        return {\r\n            ...toRefs(state),\r\n            currentTabComponent,\r\n            tabhome,\r\n            tabposts,\r\n            tabarchive\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.tab-button{\r\n    padding: 6px 10px;\r\n    border-top-left-radius:3px;\r\n    border-top-right-radius:3px;\r\n    border: 1px solid #ccc;\r\n    cursor: pointer;\r\n    background: #f0f0f0;\r\n    margin-bottom: -1px;\r\n    margin-right: -1px;\r\n}\r\n.tab-button:hover{\r\n    background: #f5d1d1;\r\n}\r\n.tab-button.active{background: #e0e0e0;}\r\n.demo-tab{border: 1px solid #ccc;padding: 10px;}\r\n</style>\r\n"],"mappings":"AAUA,SAASA,QAAQ,EAACC,MAAM,EAACC,QAAO,QAAS,KAAI;AAC7C,OAAOC,OAAM,MAAO,eAAc;AAClC,OAAOC,QAAO,MAAO,gBAAe;AACpC,OAAOC,UAAS,MAAO,kBAAiB;AAExC,eAAc;EACVC,UAAU,EAAC;IACPH,OAAO;IAACE,UAAU;IAACD;EACvB,CAAC;EACDG,KAAKA,CAAA,EAAE;IACH,MAAMC,KAAI,GAAGR,QAAQ,CAAC;MAClBS,UAAU,EAAC,CAAC;MACZC,YAAY,EAAC,CAAC,SAAS,EAAC,UAAU,EAAC,YAAY,CAAC;MAChDC,QAAQ,EAAC,CAAC,MAAM,EAAC,MAAM,EAAC,MAAM;IAClC,CAAC;IACD,MAAMC,mBAAmB,GAACV,QAAQ,CAAC,MAAI;MAAC,OAAOM,KAAK,CAACE,YAAY,CAACF,KAAK,CAACC,UAAU,CAAC;IAAA,CAAC;IACpF,OAAO;MACH,GAAGR,MAAM,CAACO,KAAK,CAAC;MAChBI,mBAAmB;MACnBT,OAAO;MACPC,QAAQ;MACRC;IACJ;EACJ;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}